// @gnrsn/dotfiles project local configuration
{
  "workspace-config": {
    "format_on_save": true,
    "allow_project_tsdk": true,
    "graphite": true,
    "custom_filetypes": {
      "pattern": {
        ".*/dot%-config/git/config": "gitconfig",
        ".*/dot%-config/git/ignore": "gitignore"
      }
    }
  }
}
